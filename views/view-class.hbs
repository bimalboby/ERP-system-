					</ul>
				  </li>
			</ul>
		</div>
	</div>

<!--Sidebar-->
	<div class="sidebar transition overlay-scrollbars animate__animated  animate__slideInLeft">
        <div class="sidebar-content"> 
        	<div id="sidebar">
			
			<!-- Logo -->
			<div class="logo">
					<h2 class="mb-0"> Principal</h2>
			</div>

            <ul class="side-menu">
               

				<!-- Divider-->
                <li class="divider" data-text="STARTER">View</li>

                <li>
                    <a href="/principal/view-students">
						<i class='bx bx-columns icon' ></i> 
						View-Students
						<i class='bx bx-chevron-right icon-right' ></i>
					</a>

					 <a href="/principal/view-teachers">
						<i class='bx bx-columns icon' ></i> 
						View-Teachers
						<i class='bx bx-chevron-right icon-right' ></i>
					</a>
						 <a href="#">
						<i class='bx bx-columns icon' ></i> 
						View-Class
						<i class='bx bx-chevron-right icon-right' ></i>
					</a>
				
					 <a href="#">
						<i class='bx bx-columns icon' ></i> 
						View-Marks
						<i class='bx bx-chevron-right icon-right' ></i>
					</a>
                   
     
                    

				<!-- Divider-->
                <li class="divider" data-text="Atrana">Add/Edit</li>

				<li>
					 <li>
					<a href="/principal/add-student" class="active">
						<i class='bx bxs-dashboard icon' ></i> Add Student
					</a>
					  <a href="/principal/add-teacher-form" class="active">
						<i class='bx bxs-dashboard icon' ></i> Add Teacher
					</a>
	
						  <a href="/principal/create-class-form" class="active">
						<i class='bx bxs-dashboard icon' ></i> Create Class
					</a>
					  <a href="/principal/add-subject-form" class="active">
						<i class='bx bxs-dashboard icon' ></i> Create subject
					</a>
		
				</li>
                
				 <li class="divider" data-text="Atrana">Messages</li>

				<li>
					 <li>
					<a href="#" class="active">
						<i class='bx bxs-dashboard icon' ></i> Sent Notifications
					</a>
					  <a href="#" class="active">
						<i class='bx bxs-dashboard icon' ></i> View Notifications
				
		
				</li>

            
        </div>

       </div> 
	 </div>
	</div><!-- End Sidebar-->



	<div class="sidebar-overlay"></div>


	<!--Content Start-->
	 <div class="content-start transition">
		<div class="container-fluid dashboard">
			
		
			

				<div class="col-md-12">
					<div class="card">
						<div class="card-header">
							<h4>Class</h4>
						</div>
						<div class="card-body"> 
						<div class="table-responsive"> 
						<table class="table table-striped">
							<thead>

							  <tr>
								
								<th scope="col">Year</th>
								<th scope="col">Section</th>
                                	<th scope="col"> Students</th>

								
							  </tr>
							</thead>
							<tbody>
                                {{#each c}}
							  <tr>
								<th scope="row">{{this.year}}	</th>
								<td>{{this.section}}</td>
								<td>{{this.studentCount}}</td>
                    
							  </tr>
                              {{/each}}

							 
							</tbody>
						  </table>
						  </div>
						</div>
					</div>
				</div>

		   </div>
		</div>
	</div> 


	<!-- Footer -->				
	<footer>
		<div class="footer">
			<div class="float-start">
				<p>2023 &copy; St Philomina</p>
			</div>
				<div class="float-end">
				
			</div>
		</div>
	</footer>


	<!-- Preloader -->
	<div class="loader">
		<div class="spinner-border text-light" role="status">
			<span class="sr-only">Loading...</span>
		</div>
	</div>
	

 </body>
</html>
