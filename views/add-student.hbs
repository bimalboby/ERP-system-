				<h1>Add-Student</h1>
				</div>
            
            <div class="row">

                <div class="col-12 col-md-6 col-lg-6">
                  <div class="card">
                  <form method="post" action="http://localhost:3000/principal/add-student">
                      <div class="card-header">
        
                      </div>
                      <div class="card-body">
                        <div class="mb-3">
                          <label>Student Name</label>
                          <input type="text"  name="name" class="form-control" required="">
                        </div>
						  <div class="mb-3">
                          <label>Mother Name</label>
             				 <input type="text" name="motherName"  class="form-control">
                        </div>

						      
						<div>
						<label>Father Name</label>
					   <input type="text" name="fatherName" class="form-control" >
						</div>
						<div>
						<label>Address</label>
					   <input type="text" name="address" class="form-control" >
						</div>
						
								<div>
						<label>Parent Ph. No</label>
					   <input type="number" name="parentph" class="form-control" >
						</div>

									<div>
						<label> Ph. No</label>
					   <input type="number" name="ph" class="form-control" >
						</div>

						<div>
									<label>Gender</label>
							   <select name="genter" class="form-control">
                    <option>---</option>
                    <option>MALE</option>
                    <option>FEMALE</option>
                    <option>NON-BINARY</option>
                </select>
						</div>

							<div>
									<label>Year</label>
							   <select name="year" class="form-control">
                    <option>---</option>
                      <option>1</option>
                    <option>2</option>
                    <option>3</option>
                    <option>4</option>
                     <option>5</option>
                    <option>5</option>
                    <option>7</option>
                       <option>8</option>
                    <option>9</option>
                    <option>10</option>
                </select>
						</div>

                        <div class="mb-3">
                          <label>Email</label>
                          <input type="email" name="email" class="form-control" required="">
                        </div>
                       
                      </div>
                      <div class="card-footer text-right">
                        <button class="btn btn-primary">Submit</button>
                      </div>
                    </form>
                </div>
